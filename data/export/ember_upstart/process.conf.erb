start on starting <%= app %>-<%= process.name %>
stop on stopping <%= app %>-<%= process.name %>
respawn
kill signal QUIT

script
  cd <%= engine.directory %>
<% engine.environment.each_pair do |var, value| %>
  export <%= var.upcase %>=<%= value %>
<% end %>
  <%= process.command %> >> <%= log_root %>/<%= process.name %>-<%= num %>.log 2>&1
end script
